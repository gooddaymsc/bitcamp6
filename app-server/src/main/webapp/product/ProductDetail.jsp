    pageEncoding="UTF-8"
    trimDirectiveWhitespaces="true" %>
    <%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!DOCTYPE html>
<html>
<head>
  <title></title>
  <link rel="stylesheet" href="../node_modules/bootstrap/dist/css/bootstrap.css">
  
  <script src="../node_modules/@popperjs/core/dist/umd/popper.js"></script>
  <script src="../node_modules/bootstrap/dist/js/bootstrap.js"></script>
  
  <style>
    .container {
        width: 640px;
    }
  </style>
</head>
<body>
<div class="container">
<h1> (MVC + EL)</h1>
<form action='update'>
<div class="mb-3 row">
  <label for='f-productNumber' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-productNumber' type='text' name='productNumber' class="form-control" value='${product.productNumber}' readonly>
  </div>
</div>
    <div class="mb-3 row">
  <label for='f-rate' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-rate' type='text' name='rate' class="form-control" value='${product.rate}' readonly>
  </div>
</div>
<div class="mb-3 row">
  <label for='f-type' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-type' type='text' name='type' class="form-control" value='${product.productType.type}' >
  </div>
</div>
<div class="mb-3 row">
  <label for='f-subType' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-subType' type='text' name='subType' class="form-control" value='${product.productType.subType}' >
  </div>
</div>
<div class="mb-3 row">
  <label for='f-countryOrigin' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-countryOrigin' type='text' name='countryOrigin' class="form-control" value='${product.countryOrigin}'>
  </div>
</div>
<c:choose> 
  <c:when test="${product.productType.type eq ''}">
    <div class="mb-3 row">
      <label for='f-variety' class="col-sm-2 col-form-label"></label>
      <div class="col-sm-6">
        <input id='f-variety' type='text' name='variety' class="form-control" value='${product.variety}' >
      </div>
    </div>
  </c:when>
</c:choose>

<div class="mb-3 row">
  <label for='f-volume' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-volume' type='text' name='volume' class="form-control" value='${product.volume}' >
  </div>
</div>
<div class="mb-3 row">
  <label for='f-alcoholLevel' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-alcoholLevel' type='text' name='alcoholLevel' class="form-control" value='${product.alcoholLevel}' >
  </div>
</div>
<div class="mb-3 row">
  <label for='f-sugarLevel' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-sugarLevel' type='text' name='sugarLevel' class="form-control" value='${product.sugarLevel}' >
  </div>
</div>
<div class="mb-3 row">
  <label for='f-acidity' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-acidity' type='text' name='acidity' class="form-control" value='${product.acidity}' >
  </div>
</div>
<div class="mb-3 row">
  <label for='f-weight' class="col-sm-2 col-form-label"></label>
  <div class="col-sm-6">
    <input id='f-weight' type='text' name='weight' class="form-control" value='${product.weight}' >
  </div>
</div>
<c:choose> 
  <c:when test="${loginUser.authority eq 8}">
    <button class="btn btn-primary"></button>
    <a href='delete?no=${product.productNumber}' class="btn btn-primary"></a> <a href='list' class="btn btn-primary"></a><br>
  </c:when>
    <c:when test="${loginUser.authority eq 4}">
    <button class="btn btn-primary"></button>
    <a href='delete?no=${product.productNumber}' class="btn btn-primary"></a> <a href='list' class="btn btn-primary"></a><br>
    <a href='../stock/form?productNumber=${product.productNumber}' class="btn btn-primary"></a><br>
  </c:when>
  <c:when test="${loginUser.authority eq 2}">
    <a href='list' class="btn btn-primary"></a><br>
  </c:when>
    <c:otherwise>
      <a href='list' class="btn btn-primary"></a><br>
    </c:otherwise>
</c:choose>
<hr />
</form>

<h4></h4>
<a href='review/form?no=${product.productNumber}' class="btn btn-primary"></a><br>
<c:forEach items="${reviewList}" var="review">
<fieldset>
<legend> : ${review.member.id}</legend>
     <p> : ${review.score}</p>
     <p> : <a href='review/detail?no=${review.no}'>${review.comment}</a></p>
     <p> : ${review.registeredDate}</p>
</fieldset>
</c:forEach>
</div> <!-- container -->
</body>
</html>
